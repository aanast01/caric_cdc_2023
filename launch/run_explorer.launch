<launch>

    <arg name="scenario" default="mbs" />
    <arg name="drone" default="raffles" />

    <group ns="$(arg drone)">
        
        <arg name="namespace"  default="$(arg drone)" />
        <param name="scenario" value="$(arg scenario)" />
        <param name="namespace" value="$(arg namespace)" />
        <param name="debug" value="true" />

	<node name="explorer_script" pkg="kios_solution" type="uav_fov.py"  output="screen"/>
    </group>
    
</launch>